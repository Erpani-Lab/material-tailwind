{
  "name": "material-tailwind",
  "description": "Material Tailwind is an easy-to-use components library for Tailwind CSS inspired by Material Design.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/material-tailwind.git"
  },
  "author": {
    "name": "sajadevo",
    "url": "https://twitter.com/sajadevo_"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/creativetimofficial/material-tailwind/issues"
  },
  "homepage": "https://material-tailwind.com",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
