#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sajad/Developer/Creative Tim/Material Tailwind/material-tailwind/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sajad/Developer/Creative Tim/Material Tailwind/material-tailwind/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/next-sitemap@3.1.44_6mcdnd6l3e73ees653pijllatu/node_modules/next-sitemap/bin/next-sitemap.mjs" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/next-sitemap@3.1.44_6mcdnd6l3e73ees653pijllatu/node_modules/next-sitemap/bin/next-sitemap.mjs" "$@"
fi
