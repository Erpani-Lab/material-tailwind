#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/sajad/Developer/Creative Tim/Material Tailwind/material-tailwind/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/sajad/Developer/Creative Tim/Material Tailwind/material-tailwind/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/turbo@1.10.13/node_modules/turbo/bin/turbo" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/turbo@1.10.13/node_modules/turbo/bin/turbo" "$@"
fi
